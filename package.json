{
  "name": "openbauth-panel",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun ./src/index.ts",
    "dev": "bun --watch ./src/index.ts",
    "typecheck": "bunx tsc --noEmit",
    "test": "bun test",
    "test:unit": "bun test tests/api/unit",
    "test:integration": "bun test tests/api/integration",
    "test:performance": "bun test tests/api/performance",
    "test:security": "bun test tests/api/security",
    "test:users": "bun test tests/users.test.ts",
    "test:all": "bun test tests/api",
    "test:coverage": "bun test --coverage",
    "setup:test-data": "bun run scripts/setup-test-data.ts",
    "init:permissions": "bun run src/scripts/init-permissions.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@hono/zod-validator": "^0.7.5",
    "hono": "^4.10.7",
    "open-bauth": "^1.3.8",
    "zod": "^4.1.13"
  }
}